.swiper-tab{  
    width: 100%;  
    border-bottom: 2rpx solid #777777;  
    text-align: center;  
    line-height: 80rpx;
}  

.swiper-tab-list{  font-size: 30rpx;  
    display: inline-block;  
    width: 50%;  
    color: #777777; 
    text-align: center; 
}  
.on{ color: #da7c0c;  
    border-bottom: 5rpx solid #da7c0c;}  
  
.swiper-box{ display: block; height: 100%; width: 100%; overflow: hidden; }  
.swiper-box view{  
    text-align: center;  
}

.viewBar{
  font-family: HYe3gj;
  font-size: 14px;
  color: #B87F9A;
  letter-spacing: 0;
  line-height: 14px;
}

.giftPrice{
  font-family: ArialRoundedMTBold;
  font-size: 17px;
  color: #F55B9E;
  letter-spacing: 0;
  line-height: 17px;
}

.giftName{
  font-family: HYe3gj;
  font-size: 14px;
  color: #513F45;
  letter-spacing: 0;
  line-height: 14px;
}

.item_content{
  width: 100%;
  height: 50px;
  font-family: HYe3gj;
  font-size: 14px;
  color: #513F45;
  letter-spacing: 0;
  line-height: 14px; 
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
}

