/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.numberFont{
  font-family: ArialRoundedMTBold;
  font-size: 17px;
  color: #F55B9E;
  letter-spacing: 0;
  line-height: 17px;
}

.titleFont{
  font-family: PingFangSC-Medium;
  font-size: 19px;
  color: #FFFFFF;
  letter-spacing: 0.5px;
  line-height: 19px;
}

.backgroundimage{
  background-image: linear-gradient(-135deg, #FF6868 0%, #D967F2 100%);
}
